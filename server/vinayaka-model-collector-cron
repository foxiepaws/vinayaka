nice /usr/local/bin/vinayaka-model-collector-impl 12 400 &
sleep 1m
nice /usr/local/bin/vinayaka-model-collector-impl 15 400 &
sleep 1m
nice /usr/local/bin/vinayaka-model-collector-impl 18 400
wait

